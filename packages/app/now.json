{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Frame-Options",
          "value" : "ALLOW-FROM *"
        },
        {
          "key" : "Content-Security-Policy",
          "value" : "frame-ancestors *"
        }
      ]
    }
  ]
}
